{
  "name": "eacs-server",
  "version": "1.0.0",
  "description": "Extensible Access Control System Server",
  "keywords": [
    "eacs",
    "acs",
    "access control",
    "user",
    "rfid"
  ],
  "main": "dist/index.js",
  "scripts": {
    "test": "mocha -r ts-node/register -r tsconfig-paths/register --recursive src/**/*.spec.ts",
    "cover": "nyc npm test",
    "start": "npm run build:live",
    "build:live": "nodemon --watch src --exec ts-node --type-check -r tsconfig-paths/register ./src/index.ts",
    "build": "tsc"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "**/*.spec.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html",
      "lcov"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chemicstry/eacs-server.git"
  },
  "author": "chemicstry <chemicstry@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chemicstry/eacs-server/issues"
  },
  "homepage": "https://github.com/chemicstry/eacs-server#readme",
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/command-line-args": "^4.0.2",
    "@types/command-line-usage": "^5.0.0",
    "@types/mocha": "^2.2.48",
    "@types/node": "^8.9.1",
    "@types/winston": "^2.3.7",
    "@types/ws": "^4.0.0",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "mocha": "^5.0.5",
    "nodemon": "^1.17.4",
    "nyc": "^11.6.0",
    "ts-node": "^4.1.0",
    "tsconfig-paths": "^3.1.1",
    "typescript": "^2.7.1"
  },
  "dependencies": {
    "@types/bluebird": "^3.5.25",
    "@types/ldapjs": "^1.0.3",
    "@types/lowdb": "^1.0.6",
    "@types/mdns": "0.0.32",
    "@types/sequelize": "^4.27.35",
    "@types/shortid": "0.0.29",
    "command-line-args": "^5.0.2",
    "command-line-usage": "^5.0.4",
    "eacs-socket": "^1.0.1",
    "jsonwebtoken": "^8.2.0",
    "lowdb": "^1.0.0",
    "mdns": "^2.4.0",
    "modular-json-rpc": "^1.0.6",
    "pg": "^7.8.0",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^4.42.0",
    "sequelize-cli": "^5.4.0",
    "sequelize-typescript": "^0.6.7",
    "shortid": "^2.2.14",
    "winston": "^2.4.0",
    "ws": "^4.0.0"
  }
}
